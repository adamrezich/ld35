        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Move to the music:


// 8 possible directions:


if (keyboard_check(ord('W'))) { 
    if (keyboard_check(ord('A')))
        move_direction = UPL;
    else if (keyboard_check(ord('D')))
        move_direction = UPR;
    else 
        move_direction = UP;
}
else if (keyboard_check(ord('S'))) {
    if (keyboard_check(ord('A')))
        move_direction = DOWNL;
    else if (keyboard_check(ord('D')))
        move_direction = DOWNR;
    else 
        move_direction = DOWN;
}
else if (keyboard_check(ord('A'))) {
    move_direction = LEFT;
}
else if (keyboard_check(ord('D'))) {
    move_direction = RIGHT;
}


switch (move_direction) {

    case RIGHT:
        target_x = x + MOVE_SPEED;
        target_y = y;
        break;

    case DOWNR:
        target_x = x + MOVE_SPEED*(2/3);
        target_y = y + MOVE_SPEED*(2/3);
        break;

    case DOWN:
        target_x = x;
        target_y = y + MOVE_SPEED;
        break;

    case DOWNL:
        target_x = x - MOVE_SPEED*(2/3);
        target_y = y + MOVE_SPEED*(2/3);
        break;

    case LEFT:
        target_x = x - MOVE_SPEED;
        target_y = y;
        break;

    case UPL:
        target_x = x - MOVE_SPEED*(2/3);
        target_y = y - MOVE_SPEED*(2/3);
        break;

    case UP:
        target_x = x;
        target_y = y - MOVE_SPEED;
        break;

    case UPR:
        target_x = x + MOVE_SPEED*(2/3);
        target_y = y - MOVE_SPEED*(2/3);
        break;
        
    default: 
        target_x = x;
        target_y = y;
}



alarm[2] = timing;
</string>
          </argument>
        </arguments>
      </action>
    </event>
